@{
    ViewBag.Title = "Log Ind";
}

<div class="jumbotron">
    <h1>BYGGEMARKED</h1>
    <h3>Log Ind</h3>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">

            <div class="form-group">
                <div class="col-md-10">
                    @Html.EditorFor(p => p, new { htmlAttributes = new { @class = "form-control" } })
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Log Ind / Opret" class="btn btn-dark" />
                </div>
            </div>
        </div>
    }
</div>
